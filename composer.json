{
    "name": "lupecat/lupecat",
    "description": "A versatile library to build dynamic web applications and APIs.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "authors": [
        {
            "name": "Jose Meira",
            "email": "jose.meira@fba-consulting.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lupecat\\": "src",
            "App\\": "app"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "server": "php -S localhost:8080 -t public public/index.php"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "slim/slim": "3.12.5"
    },
    "require": {
        "jenssegers/blade": "^1.1",
        "lupecat/render": "^1.0"
    }
}
